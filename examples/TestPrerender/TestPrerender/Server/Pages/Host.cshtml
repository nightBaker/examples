@page  "/"
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@using TestPrerender.Client
@model TestPrerender.Server.Pages.HostModel


<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />
    <title>TestPrerender</title>
    <base href="/" />
    <link href="css/bootstrap/bootstrap.min.css" rel="stylesheet" />
    <link href="css/app.css" rel="stylesheet" />
    <link href="TestPrerender.Client.styles.css" rel="stylesheet" />
</head>

<body>
    <component type="typeof(TestPrerender.Client.App)" render-mode="WebAssemblyPrerendered" />
    <div id="blazor-error-ui">
        An unhandled error has occurred.
        <a href="" class="reload">Reload</a>
        <a class="dismiss">🗙</a>
    </div>
    <script src="_framework/blazor.webassembly.js"></script>
</body>

</html>